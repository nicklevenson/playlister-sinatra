<h1>Artist</h1>
<label>Artist name: </label><%=@artist.name%><br>

<label>Songs by this artist:</label>
<ol>
<%@songs.each do |song| %>
  <li><a href="/songs/<%=song.slug%>"><%= song.name %></a></li>
<%end%>
</ol>

<label>Genres by this artist:</label>
<ol>
<%@genres.each do |genre| %>
  <li><a href="/genres/<%=genre.slug%>"><%= genre.name %></a></li>
<%end%>
</ol>
